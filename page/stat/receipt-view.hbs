<form class="form-horizontal clearfix receipt-form model-editor" method="post" enctype="multipart/form-data" id="{{invoice_id}}">
  <div class="form-group">
    <label class="control-label col-sm-2">编号：</label>
    <div class="col-sm-2">
      <p class="form-control-static">{{id}}</p>
    </div>
    <label class="control-label col-sm-2">申请日期：</label>
    <div class="col-sm-2">
      <p class="form-control-static">{{apply_time}}</p>
    </div>
    <label class="control-label col-sm-2">单位：</label>
    <div class="col-sm-2">
      <p class="form-control-static">元</p>
    </div>
  </div>

  <div class="form-group receipt-detail receipt-first-row">
    <label class="control-label col-sm-2">客户名称：</label>
    <div class="form-control-static col-sm-2">
      <p id="customer" class="form-control-static">{{full_name}}</p>
    </div>

    <div class="product col-sm-8">
      <div class="col-sm-2 text-center">
        <label class="control-label">产品名称：</label>
        <a class="btn btn-primary receipt-button" title="添加">添加</a>
      </div>
      <div class="col-sm-10"  id="productContent">
        {{#each products}}
          <div class="form-control-static col-sm-3 select-button">
            <input type="checkbox" name="products[]" value="{{ad_id}}" id="{{ad_id}}" class="ad_id" checked disabled>
            <label for="{{ad_id}}">{{ad_name}}</label>
          </div>
        {{/each}}
      </div>
    </div>
  </div>


