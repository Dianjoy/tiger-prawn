<form class="model-editor creator-form" action="{{API}}channel/" id="popup-form">
  <div class="alert alert-msg" style="display: none;"></div>
  <div class="form-group">
    <label for="alias">渠道简称</label>
    <input class="form-control" name="alias" id="alias" required>
  </div>
  <div class="form-group">
    <label for="full_name">渠道全称</label>
    <input class="form-control" name="full_name" id="full_name" required>
  </div>
  <div class="form-group">
    <label for="type">渠道类型</label>
    <select class="form-control" name="type" id="type">
      <option value="0">广告主</option>
      <option value="1">网盟</option>
      <option value="2">个人</option>
      <option value="3">换量</option>
    </select>
  </div>
</form>